    "@sveltejs/kit": "^1.24.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "eslint": "^8.49.0",
    "@vercel/analytics": "^1.1.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "tslib": "^2.6.2",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
